{
  "name": "gamegogakuen-jp",
  "version": "1.0.0",
  "description": "Language Quiz Bot for Video Game Text",
  "scripts": {
    "build:dev": "webpack --config webpack.dev.js",
    "build:staging": "webpack -p --config webpack.staging.js",
    "build:prod": "webpack -p --config webpack.prod.js",
    "deploy:staging": "echo Staging not implemented",
    "deploy:prod": "git push -f heroku deploy:master",
    "serve:dev": "npm run build:dev && npm run server",
    "serve:staging": "npm run build:staging && NO_HTTPS=true npm start",
    "serve:prod": "npm run build:prod && NO_HTTPS=true npm start",
    "server": "node --require dotenv/config ./dist/server.js",
    "shipit": "git checkout -b deploy && rm .gitignore && echo 'node_modules\n.env*\n__DBScripts\ncoverage' > .gitignore && npm run build:prod && pushd ../GG-UI && npm run build:prod && popd && git add . && git commit -m 'build' && npm run deploy:prod && git checkout master && git branch -D deploy",
    "stageit": "npm run build:staging && pushd ../GG-UI && npm run build:staging && popd && npm run deploy:staging",
    "serveit": "npm run build:dev && pushd ../GG-UI && npm run build:dev && popd && npm start",
    "start": "node ./dist/server.js",
    "standard": "standard || exit 0",
    "test": "jest --runInBand",
    "test:cov": "jest --runInBand --collectCoverage",
    "test:watch": "jest --runInBand --watch"
  },
  "author": "appleJax <appleJax@users.noreply.github.com>",
  "license": "MIT",
  "engines": {
    "node": "^10.20.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "connect-redis": "^3.3.3",
    "core-js": "^3.6.5",
    "express": "^4.16.2",
    "express-authorization": "^1.0.0",
    "express-redis-cache": "^1.1.1",
    "express-session": "^1.15.6",
    "mime-types": "^2.1.18",
    "mongoose": "^5.0.14",
    "mongoose-long": "^0.2.1",
    "multer": "^1.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pngjs2": "^2.0.0",
    "pug": "^2.0.3",
    "redis": "^2.8.0",
    "regenerator-runtime": "^0.13.7",
    "serve-static": "^1.13.2",
    "source-map-support": "^0.5.3",
    "twit": "^2.2.9",
    "unzip-stream": "^0.3.0",
    "urlencode": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-jest": "^22.2.2",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.3.1",
    "dotenv": "^4.0.0",
    "jest-cli": "^22.2.2",
    "jest-environment-node": "^22.4.3",
    "mongodb-memory-server": "^1.7.0",
    "standard": "^11.0.1",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "globalSetup": "./test-env/setup.js",
    "globalTeardown": "./test-env/teardown.js",
    "testEnvironment": "./test-env/mongo-env.js",
    "setupFiles": [
      "./test-env/env-vars.js"
    ],
    "moduleNameMapper": {
      "^Anki(.*)$": "<rootDir>/src/anki$1",
      "^Config(.*)$": "<rootDir>/src/config$1",
      "^DB(.*)$": "<rootDir>/src/db$1",
      "^Models(.*)$": "<rootDir>/src/db/models$1",
      "^TestUtils(.*)$": "<rootDir>/test-env/utils$1",
      "^Src(.*)$": "<rootDir>/src$1",
      "^Twitter(.*)$": "<rootDir>/src/twitter$1",
      "^Utils(.*)$": "<rootDir>/src/utils$1"
    }
  }
}
