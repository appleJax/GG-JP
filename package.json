{
  "name": "GameGogakuenJP",
  "version": "1.0.0",
  "description": "Language Quiz Bot for Video Game Text",
  "scripts": {
    "build": "webpack --config webpack.dev.js",
    "deploy": "webpack -p --config webpack.prod.js",
    "server": "node --require dotenv/config ./dist/server.js",
    "devstart": "npm run build && npm run server",
    "start": "node ./dist/server.js",
    "test": "jest --runInBand",
    "testCov": "jest --runInBand --collectCoverage",
    "testWatch": "jest --runInBand --watch"
  },
  "author": "appleJax <appleJax@users.noreply.github.com>",
  "license": "MIT",
  "engines": {
    "node": "^9.5.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.39",
    "body-parser": "^1.18.2",
    "connect-redis": "^3.3.3",
    "express": "^4.16.2",
    "express-authorization": "^1.0.0",
    "express-redis-cache": "^1.1.1",
    "express-session": "^1.15.6",
    "mongoose": "^5.0.14",
    "multer": "^1.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pngjs2": "^2.0.0",
    "pug": "^2.0.3",
    "redis": "^2.8.0",
    "source-map-support": "^0.5.3",
    "twit": "^2.2.9",
    "unzip-stream": "^0.3.0",
    "urlencode": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.39",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.2.2",
    "babel-loader": "^8.0.0-beta.0",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.3.1",
    "dotenv": "^4.0.0",
    "jest-cli": "^22.2.2",
    "jest-environment-node": "^22.4.3",
    "mongodb-memory-server": "^1.7.0",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "globalSetup": "./test-env/setup.js",
    "globalTeardown": "./test-env/teardown.js",
    "testEnvironment": "./test-env/mongo-env.js",
    "setupFiles": [
      "./test-env/env-vars.js"
    ],
    "moduleNameMapper": {
      "^Anki(.*)$": "<rootDir>/src/anki$1",
      "^Config(.*)$": "<rootDir>/src/config$1",
      "^DB(.*)$": "<rootDir>/src/db$1",
      "^Models(.*)$": "<rootDir>/src/db/models$1",
      "^TestUtils(.*)$": "<rootDir>/test-env/utils$1",
      "^Src(.*)$": "<rootDir>/src$1",
      "^Twitter(.*)$": "<rootDir>/src/twitter$1",
      "^Utils(.*)$": "<rootDir>/src/utils$1"
    }
  }
}
